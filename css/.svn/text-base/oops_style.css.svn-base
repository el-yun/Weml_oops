#top {
	background:black;
	height:4px;
	border-bottom:1px dashed white;
}

.topbar-container {
	height: 36px;
	background: url(../images/bg_topnavigation.jpg) repeat-x;
	width: 100%;
}
.topbar-container .member {
	float: right;
	padding-right: 16px;
	margin-top:8px;
}

.topbar-container .member ul li {
	float: left;
	display: block;
	margin: 0px 20px;
}





/* navigation */

header {
	padding-top: 5px;
	border-bottom: 1px dotted #CCC;
	padding-bottom: 5px;
	/* [disabled]overflow: hidden; */
	height: 64px;
	width: 998px;
	margin: 0 auto;
	clear: both;
}
.logo {
	height: 64px;
	width: 176px;
	background: url(../images/adinath-web-solutions-logo.png) no-repeat;
	position: absolute;
}

header #topnav {
	float: right;
	/*background: url(images/navigation-bg.png) no-repeat;*/
	margin: 0px;
	padding: 25px 0px 0px;
}



#topnav ul li {
	float: left;
	display: block;
	/* [disabled]background: url(../images/left-border.gif) no-repeat left top; */
	margin: 0px 20px;
}

#topnav ul li a {
	float: left;
	display: block;
	color: #000;
	text-decoration: none;
	font: 13px 'BankGothicLtBTLight', sans-serif;
	padding-bottom: 31px;
	margin: 0px;
}

#topnav ul li a.active {
	color: #0092CC;
	border-bottom: 1px solid #0092CC;
}

#topnav ul li.topnav-lastchild {
	margin-right: 0px;
}

#topnav ul li {
	float: left;
	display: block;
	margin: 0px 20px;
}

a:visited {
/* [disabled]color: #0092CC; */
}


#intro {
	height: 180px;
	width: 968px;
	margin: 10px auto 0px;
	background: url(../images/banner-bg.png) no-repeat;
	padding: 15px;
}

footer {
	width: 998px;
	margin: 0px auto;
}

#bottom {
	padding-top: 10px;
	overflow: hidden;
	border-top: 1px solid #999;
	padding-bottom: 60px;
}

#bottom ul .bottommenu {
	float: left;
	display: block;
}

#bottom ul li ul li {
	float: left;
}

#bottom ul .bottommenu ul li a {
	float: left;
	display: block;
	padding-left: 10px;
	padding-right: 10px;
	border-left: 1px solid #CCC;
	color: #333;
}

#bottom ul .bottommenu ul li:first-child a {
	border: none;
	padding-left: 0px;
}



#bottom ul .bottomlogo {
	float: right;
	display: block;
}

#bottom ul .bottommenu p {
	font: 12px 'HelveticaCondensedRegular', sans-serif;
	display: block;
	margin: 0px;
	padding: 5px 0px 0px;
	clear: both;
}



.footermail {
color: #0092CC;
}

article {
	width: 998px;
	margin: 0 auto;
}